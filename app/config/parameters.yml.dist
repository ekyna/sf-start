parameters:
    hostname:          ~
    docker_name:       start

    database_driver:   pdo_mysql
    database_host:     "mysql.%docker_name%"
    database_port:     3306
    database_name:     ~
    database_user:     ~
    database_password: ~

    mailer_transport:  smtp
    mailer_host:       127.0.0.1
    mailer_port:       ~
    mailer_encryption: ~
    mailer_auth_mode:  ~
    mailer_user:       ~
    mailer_password:   ~

    locale:            fr
    locales:           [fr, en]
    secret:            ThisTokenIsNotSoSecretChangeIt

    cache:             file_system
    wkhtmltopdf_path:  /usr/bin/wkhtmltopdf

    debug_toolbar:          true
    debug_redirects:        false

    reverse_proxy.enable: false
    reverse_proxy.host: 127.0.0.1
    reverse_proxy.port: 80

    router.request_context.host: "%hostname%"
    router.request_context.scheme: http
    router.request_context.base_url: ~

    redis.host: "redis.%docker_name%"

    elasticsearch.host: "elasticsearch.%docker_name%"
    elasticsearch.port: 9030
    elasticsearch.index_name: "%hostname%"

    error_report_mail: support@ekyna.com
